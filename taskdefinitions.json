{
  "family": "WebappTD",
  "containerDefinitions": [
    {
      "name": "web",
      "image": "950196620421.dkr.ecr.us-east-1.amazonaws.com/webapp:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000
        }
      ],
      "environment": [
        {
          "name": "ENV_VAR_NAME",
          "value": "your_value"
        }
      ]
    }
  ]
}
